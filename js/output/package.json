{
  "name": "@cs124/jeed-output",
  "version": "2022.2.1",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "checker": "yarn depcheck && yarn prettier && yarn eslint && yarn tsc",
    "clean": "rimraf dist",
    "depcheck": "depcheck --ignores=prettier-plugin-*",
    "eslint": "eslint src/**",
    "ncu": "ncu",
    "prepublish": "rimraf dist && tsc",
    "start": "yarn watch",
    "test": "JEED_SERVER=http://localhost:8888 ts-mocha --paths tests/index.spec.ts",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cs124/jeed-types": "2022.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "depcheck": "1.4.3",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "npm-check-updates": "12.5.10",
    "prettier": "2.6.2",
    "prettier-package-json": "2.6.3",
    "prettier-plugin-organize-imports": "2.3.4",
    "rimraf": "3.0.2",
    "runtypes": "6.5.1",
    "typescript": "4.6.3"
  }
}
