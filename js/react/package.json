    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "npm-check-updates": "12.4.0",
    "prettier": "2.5.1",
    "prettier-package-json": "2.6.3",
    "prettier-plugin-organize-imports": "2.3.4",
