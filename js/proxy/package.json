{
  "private": true,
  "name": "proxy",
  "version": "2022.2.1",
  "scripts": {
    "build": "rimraf dist && esbuild src/index.ts --sourcemap --bundle --platform=node --outfile=dist/index.js",
    "clean": "rimraf dist",
    "predocker:build": "yarn build",
    "docker:build": "docker build . -t cs124/jeed-proxy:latest",
    "postdocker:build": "yarn docker:tag",
    "docker:push": "docker push cs124/jeed-proxy:latest && docker push cs124/jeed-proxy:$npm_package_version",
    "docker:tag": "docker tag cs124/jeed-proxy:latest cs124/jeed-proxy:$npm_package_version",
    "eslint": "eslint src/**",
    "ncu": "ncu",
    "start": "nodemon",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write"
  },
  "dependencies": {
    "@cs124/jeed-types": "2022.2.1",
    "@cs124/koa-google-login": "2021.11.0",
    "@koa/cors": "3.1.0",
    "@koa/router": "10.1.1",
    "@panva/hkdf": "1.0.1",
    "fetch-retry": "5.0.1",
    "google-auth-library": "7.14.0",
    "isomorphic-fetch": "3.0.0",
    "jose": "4.5.1",
    "koa": "2.13.4",
    "koa-body": "4.2.0",
    "koa-ratelimit": "5.0.1",
    "lodash": "4.17.21",
    "mongodb": "4.4.0",
    "mongodb-uri": "0.9.7",
    "runtypes": "6.5.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/assert": "1.5.6",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/koa": "2.13.4",
    "@types/koa-ratelimit": "4.2.4",
    "@types/koa__cors": "3.1.1",
    "@types/koa__router": "8.0.11",
    "@types/lodash": "4.14.179",
    "@types/mongodb-uri": "0.9.1",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "babel-eslint": "10.1.0",
    "env-cmd": "10.1.0",
    "esbuild": "0.14.23",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.15",
    "npm-check-updates": "12.5.0",
    "prettier": "2.5.1",
    "prettier-package-json": "2.6.3",
    "ts-node": "10.5.0",
    "typescript": "4.6.2"
  },
  "nodemonConfig": {
    "watch": [
      "package.json",
      "src/index.ts",
      ".env.development"
    ],
    "ext": "*.ts",
    "exec": "env-cmd -f .env.development ts-node src/index.ts"
  }
}
